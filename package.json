{
  "name": "lambda",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "node esbuild.js",
    "start": "node dist/server.mjs",
    "dev:node": "node --watch dist/server.js",
    "dev:tsc": "tsc --watch --preserveWatchOutput",
    "dev:esbuild": "esbuild src/server.ts --bundle --platform=node --outfile=dist/server.js --format=cjs --watch",
    "dev": "run-p dev:*",
    "prepare-husky": "husky install && yarn husky add .husky/pre-commit \"npx lint-staged\"",
    "lint": "tsc --noemit && eslint . --ext=js --ext=ts",
    "prettify": "prettier --write .",
    "db:pull": "drizzle-kit introspect:pg && ncp ./drizzle/migrations/schema.ts ./drizzle/schema.ts",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "node drizzle/migrations/migrate.js"
  },
  "lint-staged": {
    "*.js": [
      "yarn lint",
      "yarn prettify"
    ],
    "*.ts": [
      "yarn lint",
      "yarn prettify"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@codegenie/serverless-express": "^4.13.0",
    "@supercharge/request-ip": "^1.2.0",
    "cors": "^2.8.5",
    "drizzle-orm": "^0.30.1",
    "express": "^4.18.3",
    "helmet": "^7.1.0",
    "postgres": "^3.4.3",
    "winston": "^3.12.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.529.1",
    "@types/aws-lambda": "^8.10.136",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.25",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.20.14",
    "esbuild": "^0.20.1",
    "esbuild-plugin-copy": "^2.1.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "ts-node-dev": "^2.0.0",
    "ts-patch": "^3.1.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.2",
    "typescript-transform-paths": "^3.4.7",
    "zip-a-folder": "^3.1.6"
  }
}
